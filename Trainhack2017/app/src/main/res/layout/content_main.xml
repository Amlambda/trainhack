<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.isisochbast.trainhack2017.MainActivity"
    tools:showIn="@layout/activity_main">


    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/layout_bom_SO"
        android:background="?attr/colorPrimary"
        android:layout_width="313dp"
        android:layout_height="114dp"
        android:orientation="vertical"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/layout_bom_SP"
        app:layout_constraintRight_toRightOf="@+id/layout_bom_SP"
        android:layout_marginTop="60dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="27dp"
        app:layout_constraintLeft_toLeftOf="@+id/layout_bom_SP"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintHorizontal_bias="1.0">

        <TextView
            android:id="@+id/text_view_bom_SO"
            android:layout_width="95dp"
            android:layout_height="20dp"
            android:text="@string/string_bom_SO"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="16dp" />

        <TextView
            android:id="@+id/text_view_info_down_bomSO"
            android:layout_width="189dp"
            android:layout_height="20dp"
            android:textAllCaps="true"
            android:textSize="14sp"
            android:textStyle="bold"
            android:text="hoooho"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="57dp"
            android:layout_marginRight = "@dimen/text_horizontal_margin"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            tools:layout_editor_absoluteY="58dp"
            tools:layout_editor_absoluteX="24dp" />
        <TextView
            android:id="@+id/text_view_info_up_bomSO"
            android:layout_width="186dp"
            android:layout_height="19dp"
            android:textAllCaps="true"
            android:textSize="14sp"
            android:textStyle="bold"
            android:text="hiiiiho"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="79dp"
            android:layout_marginRight = "103dp" />
        <TextView
            android:id="@+id/text_view_time_bomSO"
            android:layout_width="56dp"
            android:layout_height="21dp"
            android:text="  3 min"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="#FFFFF0"
            app:layout_constraintTop_toTopOf="parent"
            android:background="?attr/colorPrimaryDark"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="55dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            tools:layout_editor_absoluteX="227dp"
            tools:layout_editor_absoluteY="65dp" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/layout_bom_SP"
        android:background="?attr/colorPrimary"
        android:layout_width="311dp"
        android:layout_height="110dp"
        android:orientation="vertical"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="261dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="37dp">

        <TextView
            android:id="@+id/text_view_bom_SP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string_bom_SP"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="16dp" />


        <TextView
            android:id="@+id/text_view_info_bomSP"
            android:layout_width="188dp"
            android:layout_height="22dp"
            android:textAllCaps="true"
            android:textSize="14sp"
            android:textStyle="bold"
            android:text="hoooho"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="52dp"
            android:layout_marginRight = "@dimen/text_horizontal_margin"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            tools:layout_editor_absoluteX="24dp" />
        <TextView
            android:id="@+id/text_view_info_up_bomSO"
            android:layout_width="186dp"
            android:layout_height="19dp"
            android:textAllCaps="true"
            android:textSize="14sp"
            android:textStyle="bold"
            android:text="hiiiiho"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="72dp"
            android:layout_marginRight = "102dp" />
        <TextView
            android:id="@+id/text_view_time_bomSO"
            android:layout_width="56dp"
            android:layout_height="21dp"
            android:text="  3 min"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="#FFFFF0"
            app:layout_constraintTop_toTopOf="parent"
            android:background="?attr/colorPrimaryDark"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="55dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            tools:layout_editor_absoluteX="227dp"
            tools:layout_editor_absoluteY="65dp" />
    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/add_button"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ny bom"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="60dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/image_view_bom"
        android:layout_width="122dp"
        android:layout_height="124dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteY="371dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="vertical"
        app:layout_constraintGuide_end="385dp" />
</android.support.constraint.ConstraintLayout>
